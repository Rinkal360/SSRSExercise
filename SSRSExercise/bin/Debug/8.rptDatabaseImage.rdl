          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProductNumber">
          <DataField>ProductNumber</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ThumbNailPhoto">
